{
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv",
    "**/venv",
    "**/.git",
    "**/.pytest_cache",
    "**/.ruff_cache",
    "**/node_modules",
    "**/dist",
    "**/build",
    "**/*.egg-info",
    "**/htmlcov",
    "**/.coverage",
    "**/docs/_build",
    "**/site",
    "**/.tox",
    "**/.nox"
  ],
  "pythonVersion": "3.14",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "typeCheckingMode": "standard"
}
